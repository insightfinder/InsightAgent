[general]
namespace = insightfinder

[prometheus]
## prometheus info
# required
endpoint = http://localhost:9090
# Basic Authentication
user =
password =
# True or False
verify_certs =
# path to ca bundle
ca_certs =
# path to client cert
client_cert =
# path to client key
client_key =
# The name space needed to monitor: "kube-system, insightfinder"
namespace = insightfinder

[insightfinder]
user_name = user
license_key =
token =

# Name of the project created in the InsightFinder UI.
project_name =

# Prefix of the project name
project_name_prefix =

# Name of system owned by project
system_name = maoyu-test-k8s-monitor

# metric, metricreplay, log, logreplay, alert, alertreplay, incident, incidentreplay, deployment, deploymentreplay, trace, tracereplay
project_type = metric
run_interval = 1
indexing=false

if_url = https://stg.insightfinder.com
