#dataFormat JSON or String, if it is string need to set dataFormatRegex
insight-finder.dataFormat=String
insight-finder.dataFormatRegex=^cs\\.\\|(?<project>\\w+)\\|\\.\\w+\\.\\w+\\.\\w+\\.(?<instance>\\w+\\-\\w+)\.(?<metric>.*) (?<value>[-\\d\\.]+) (?<timestamp>\\d+)
