# Configuration for InsightFinder Zone Data Extraction

# InsightFinder credentials
insightfinder_url = "https://stg.insightfinder.com"  # or https://app.insightfinder.com
insightfinder_username = "your_insightfinder_username"
insightfinder_password = "your_insightfinder_password"
license_key = "your_insightfinder_license_key"

# Project configuration
insightfinder_project = "your_project_name"

# User agent for API requests
user_agent = "Mozilla/5.0 (compatible; InsightFinderClient/1.0;)"

# Optional: timezone offset in milliseconds (default: -18000000 for EST)
tz_offset = -18000000
